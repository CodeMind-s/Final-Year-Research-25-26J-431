syntax = "proto3";
package logs;

import "google/protobuf/timestamp.proto";

service LogsService {
  rpc GetLogById(GetLogByIdRequest) returns (GetLogResponse);
}

message GetLogByIdRequest {
  string logId = 1;
}

export class GetLogResponse {
  bool success = 1;
  string message = 2;
  string log = 3 ;
}